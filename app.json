{
  "expo": {
    "name": "BidX",
    "slug": "bidx",
    "owner": "bidx-dev",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bidx-mobile-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.bidx.client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_white.png",
        "backgroundColor": "#13144A"
      },
      "package": "io.bidx.client",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/logo_white.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_white.png",
          "resizeMode": "cover",
          "backgroundColor": "#13144A",
          "enableFullScreenImage_legacy": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4dc02473-a97c-43d9-a785-44c886a395fb"
      }
    }
  }
}
